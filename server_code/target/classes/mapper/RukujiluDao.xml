<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cl.dao.RukujiluDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.cl.entity.RukujiluEntity" id="rukujiluMap">
        <result property="rukubianhao" column="rukubianhao"/>
        <result property="yaopinmingcheng" column="yaopinmingcheng"/>
        <result property="kucun" column="kucun"/>
        <result property="rukushijian" column="rukushijian"/>
        <result property="rukubeizhu" column="rukubeizhu"/>
    </resultMap>

    <select id="selectListView"
		resultType="com.cl.entity.view.RukujiluView" >

		SELECT  rukujilu.* FROM rukujilu  rukujilu 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.cl.entity.view.RukujiluView" >
		SELECT * FROM rukujilu  rukujilu <where> 1=1 ${ew.sqlSegment}</where>
	</select>



</mapper>
